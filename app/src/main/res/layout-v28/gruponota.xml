<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/gruponota"
    android:layout_width="1100dp"
    android:layout_height="1140dp"
    android:layout_marginStart="32dp"
    android:layout_marginTop="32dp">

    <ImageView
        android:id="@+id/bloconota"
        android:layout_width="185dp"
        android:layout_height="161dp"
        android:background="@android:color/transparent"
        android:clickable="true"
        android:focusable="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.017"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.157"
        app:srcCompat="@drawable/bloco" />

    <ImageView
        android:id="@+id/favoritoempty"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_marginBottom="16dp"
        android:clickable="true"
        android:focusable="true"
        app:layout_constraintBottom_toBottomOf="@+id/bloconota"
        app:layout_constraintEnd_toEndOf="@+id/bloconota"
        app:layout_constraintHorizontal_bias="0.139"
        app:layout_constraintStart_toStartOf="@+id/bloconota"
        app:srcCompat="@drawable/favorito" />

    <ImageView
        android:id="@+id/favselecionado"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_marginBottom="16dp"
        android:clickable="false"
        android:focusable="false"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/bloconota"
        app:layout_constraintEnd_toEndOf="@+id/bloconota"
        app:layout_constraintHorizontal_bias="0.133"
        app:layout_constraintStart_toStartOf="@+id/bloconota"
        app:srcCompat="@drawable/favoritosred" />

    <ImageView
        android:id="@+id/lockclose"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:clickable="false"
        android:focusable="false"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/bloconota"
        app:layout_constraintEnd_toEndOf="@+id/bloconota"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="@+id/bloconota"
        app:layout_constraintTop_toTopOf="@+id/bloconota"
        app:layout_constraintVertical_bias="0.113"
        app:srcCompat="@drawable/cadeado" />

    <ImageView
        android:id="@+id/lockopen"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:clickable="true"
        android:focusable="true"
        app:layout_constraintBottom_toBottomOf="@+id/bloconota"
        app:layout_constraintEnd_toEndOf="@+id/bloconota"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="@+id/bloconota"
        app:layout_constraintTop_toTopOf="@+id/bloconota"
        app:layout_constraintVertical_bias="0.12"
        app:srcCompat="@drawable/cadeadoaberto" />

    <ImageView
        android:id="@+id/imageView18"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:clickable="true"
        android:focusable="true"
        app:layout_constraintBottom_toBottomOf="@+id/bloconota"
        app:layout_constraintEnd_toEndOf="@+id/bloconota"
        app:layout_constraintHorizontal_bias="0.89"
        app:layout_constraintStart_toStartOf="@+id/bloconota"
        app:layout_constraintTop_toTopOf="@+id/bloconota"
        app:layout_constraintVertical_bias="0.879"
        app:srcCompat="@drawable/copiar" />

    <TextView
        android:id="@+id/TituloPrincipal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="68dp"
        android:text="TextView"
        android:textSize="24sp"
        android:textFontWeight="700"
        app:layout_constraintBottom_toBottomOf="@+id/bloconota"
        app:layout_constraintEnd_toEndOf="@+id/bloconota"
        app:layout_constraintHorizontal_bias="0.488"
        app:layout_constraintStart_toStartOf="@+id/bloconota" />


</androidx.constraintlayout.widget.ConstraintLayout>
